  then
    local cmd="$(__macdaily_comp_prev_swch)"
    if [[ ${cmd} == --packages ]]
    then
      local vals="$(__macdaily_gem_installed)"
      __macdaily_comp "${vals}"
      return
    else
      __macdaily_comp "${opts} ${keys}"
      return
    fi
  fi
}

