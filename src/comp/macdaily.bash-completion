      --brew)
        local vals="$(__macdaily_brew_installed)"
        __macdaily_comp_installed_rev "${vals}"
        return
        ;;
      --cask)
        local vals="$(__macdaily_cask_installed)"
        __macdaily_comp_installed_rev "${vals}"
        return
        ;;
    esac
