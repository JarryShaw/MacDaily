  local cur prev opts cmds keys
  cur="${COMP_WORDS[COMP_CWORD]}"
  prev="${COMP_WORDS[COMP_CWORD-1]}"
  opts="--help --version --quiet --verbose --no-cleanup --yes"
  keys="--packages --install"

  if [[ ${prev} =~ "--help|--version" ]]
  then
